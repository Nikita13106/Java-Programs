import java.util.Scanner;
public class fibonacci_series {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter the limit");
		int l=sc.nextInt();
		int a=0,b=1;
		System.out.print(a+" " +b+" ");
		for(int i=0;i<=l;i++) {
			int sum=a+b;
			System.out.print(sum+" ");
			a=b;
			b=sum;
		}

		
	}

}
