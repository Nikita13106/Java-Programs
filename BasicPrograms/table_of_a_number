import java.util.Scanner;
public class table_of_a_number {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("ENTER A NUMBER WH0'S TABLE YOU WANT");
		int num=sc.nextInt();
		for(int i=1;i<=10;i++) {
			int ans = num*i;
			System.out.println(num+ " x "+ i+" = "+ans);
		}
	}

}
